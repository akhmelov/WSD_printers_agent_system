<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?scenebuilder-preview-i18n-resource ../../../../lang_en.properties?>
<?scenebuilder-stylesheet ../../../../main.css?>

<StackPane fx:id="root" maxHeight="-1.0" maxWidth="-1.0" prefHeight="394.0" prefWidth="594.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wsd.printers.agent.springfx.gui.SpecifyAgentPresentation">
    <children>
        <VBox alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" spacing="10.0" StackPane.alignment="CENTER">
            <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="CongifChooseButton" mnemonicParsing="false" onAction="#configChooseButtonClick" text="Open agent" />
               </children>
            </HBox>
                <HBox alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" spacing="10.0" VBox.vgrow="ALWAYS">
                    <children>
                  <Button fx:id="documentManageButton" disable="true" mnemonicParsing="false" onAction="#documentManageButtonClick" text="%specify.agent.button.nextview" />
                  <Button fx:id="agentStateButton" disable="true" mnemonicParsing="false" onAction="#agentStateButtonClick" text="Agent state" />
                    </children>
                </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" />
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
    </padding>
</StackPane>
