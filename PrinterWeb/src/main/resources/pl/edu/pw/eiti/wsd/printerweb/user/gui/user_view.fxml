                        <Image url="@printer-icon.png" />
                     </image>
                     <effect>
                        <DropShadow />
                     </effect>
                     <BorderPane.margin>
                        <Insets bottom="10.0" left="20.0" right="20.0" />
                     </BorderPane.margin>
                  </ImageView>
               </right>
               <VBox.margin>
                  <Insets left="20.0" right="20.0" />
               </VBox.margin>
            </BorderPane>
            <BorderPane maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
