                    <columns>
                      <TableColumn fx:id="nameColumn" prefWidth="101.0" text="Plik" />
                      <TableColumn fx:id="statusColumn" prefWidth="96.0" text="Status" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </left>
               <VBox.margin>
                  <Insets bottom="5.0" left="20.0" right="20.0" />
